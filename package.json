{
	"name": "@inagora/wd-view",
	"version": "1.2.25",
	"description": "基于Vue3和Typescript的UI组件库",
	"main": "dist/index.js",
	"scripts": {
		"start": "webpack serve --config webpack.dev.config.js --mode development --env development --profile --progress",
		"build": "webpack --config webpack.prod.config.js --mode production --env production",
		"test:unit": "vue-cli-service test:unit",
		"docs:dev": "vuepress dev docs",
		"docs:build": "vuepress build docs",
		"lint": "eslint --fix --ext .ts,.tsx,.vue src --quiet",
		"prepare": "husky install"
	},
	"keywords": [
		"UI",
		"vue3",
		"vue",
		"Typescript",
		"framework",
		"components",
		"ant-design-vue"
	],
	"author": "lijieniu",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.12.17",
		"@babel/plugin-transform-runtime": "^7.12.17",
		"@babel/preset-env": "^7.12.17",
		"@babel/preset-react": "^7.18.6",
		"@commitlint/cli": "^17.6.5",
		"@commitlint/config-conventional": "^17.6.5",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"@vue/babel-plugin-jsx": "^1.1.1",
		"@vue/babel-preset-jsx": "^1.4.0",
		"@vue/cli-plugin-babel": "~5.0.0",
		"@vue/cli-plugin-eslint": "~5.0.0",
		"@vue/cli-plugin-typescript": "~5.0.0",
		"@vue/cli-service": "~5.0.0",
		"@vue/compiler-sfc": "^3.0.5",
		"@vue/eslint-config-standard": "^6.1.0",
		"@vue/eslint-config-typescript": "^9.1.0",
		"@vue/runtime-dom": "^3.3.4",
		"@vue/test-utils": "2.0.0-alpha.0",
		"babel-loader": "^8.2.2",
		"babel-plugin-import": "^1.13.3",
		"clean-webpack-plugin": "^3.0.0",
		"commitlint": "^17.6.5",
		"copy-webpack-plugin": "^7.0.0",
		"css-loader": "^5.0.2",
		"css-minimizer-webpack-plugin": "^3.2.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^5.1.0",
		"eslint-plugin-vue": "^8.0.3",
		"html-webpack-plugin": "^5.2.0",
		"husky": "^8.0.3",
		"less": "^4.1.1",
		"less-loader": "^8.0.0",
		"lint-staged": "^13.2.2",
		"mini-css-extract-plugin": "^1.3.8",
		"style-loader": "^2.0.0",
		"ts-loader": "^9.2.6",
		"typescript": "~4.5.5",
		"vue": "^3.2.29",
		"vue-loader": "^17.0.0",
		"vuepress": "2.0.0-beta.27",
		"webpack": "^5.23.0",
		"webpack-cli": "^4.5.0",
		"webpack-dev-server": "^3.11.2"
	},
	"dependencies": {
		"@babel/runtime": "^7.12.18",
		"@popperjs/core": "^2.11.0",
		"ant-design-vue": "^3.2.15",
		"async-validator": "^3.4.0",
		"lodash": "^4.17.21",
		"mitt": "^2.1.0",
		"vue3-popper": "^1.4.1"
	},
	"files": [
		"dist/index.js",
		"dist/index.css"
	],
	"directories": {
		"doc": "docs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/inagora/wd-view.git"
	},
	"bugs": {
		"url": "https://github.com/inagora/wd-view/issues"
	},
	"homepage": "https://github.com/inagora/wd-view#readme",
	"lint-staged": {
		"**/*.{vue,js,jsx,tsx,ts}": [
			"npm run lint:script",
			"git add ."
		],
		"**/*.{scss}": [
			"npm run lint:style",
			"git add ."
		]
	}
}
